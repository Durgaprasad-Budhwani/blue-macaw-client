{
  "name": "blue-macaw-client",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/durgaprasad-budhwani/blue-macaw-client.git"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.25",
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.0.0",
    "gh-pages": "^2.0.1",
    "history": "^4.7.2",
    "jss": "^9.8.7",
    "lodash": "^4.17.11",
    "material-ui": "^0.20.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-loading-skeleton": "^1.0.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.2",
    "redux-socket.io": "^1.4.0",
    "reselect": "^4.0.0",
    "socket.io-client": "^2.2.0"
  },
  "scripts": {
    "start": "sh -ac '. .env.dev; react-scripts start'",
    "build": "sh -ac '. ./.env.${REACT_APP_ENV}; react-scripts build'",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "build:uat": "REACT_APP_ENV=uat npm run build",
    "build:production": "REACT_APP_ENV=production npm run build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --env=jsdom --ci --runInBand --coverage --colors",
    "eject": "react-scripts eject",
    "docs": "react-doc-generator src -o DOCUMENTATION.md",
    "deploy": "gh-pages -d build",
    "format:js": "prettier --config .prettierrc \"src/**/*.js\" --write",
    "lint:js": "eslint --config .eslintrc.json \"src/**/*.js\"",
    "flow": "flow"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-eslint": "9.x",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "7.x",
    "flow-bin": "^0.88.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react-doc-generator": "^1.2.5",
    "redux-devtools-extension": "^2.13.7"
  }
}
